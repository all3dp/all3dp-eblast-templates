        <mj-text height="50px">&nbsp;</mj-text>
      </mj-column>
    </mj-section>

    <mj-section mj-class="content content--dark">
      <mj-column mj-class="content__wrapper" border-radius="0px 0px 50px 50px" background-color="#fff">

        <mj-text color="#000" font-size="28px" align="center" font-weight="bold" css-class="features_headline">Freegan roof party!</mj-text>

        <mj-spacer height="30px" />

