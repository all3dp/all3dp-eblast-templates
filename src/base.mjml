        </mj-text>
        <mj-spacer height="50px" />
      </mj-column>
    </mj-section>

    <mj-section mj-class="content content--dark">
      <mj-column mj-class="content__wrapper">
        <mj-image width="120px" height="38px" align="center" src="https://via.placeholder.com/120x38/50" alt="Logo" padding-bottom="20px" padding-top="20px" padding-top="0" css-class="footer_logo"></mj-image>
