          <mj-social-element name="instagram" href="https://example.com/" target="_blank" alt="instagram" background-color="transparent" src="https://sf-asset-manager.s3.amazonaws.com/96363/590/4359.png"></mj-social-element>
          <mj-social-element name="linkedin" href="https://example.com/" target="_blank" alt="linkedin" background-color="transparent" src="https://sf-asset-manager.s3.amazonaws.com/96363/590/4360.png"></mj-social-element>
