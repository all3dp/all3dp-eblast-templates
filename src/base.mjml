      </mj-selector>

      <mj-selector path=".nav_claim div span">
        <mj-html-attribute name="mc:edit">nav_claim</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".hero_headline div">
        <mj-html-attribute name="mc:edit">hero_headline</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".hero_image img">
        <mj-html-attribute name="mc:edit">hero_image</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".desc_headline div">
        <mj-html-attribute name="mc:edit">desc_headline</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".desc_keypoints div">
        <mj-html-attribute name="mc:edit">desc_keypoints</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".desc_intro div">
        <mj-html-attribute name="mc:edit">desc_intro</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".desc_content div">
        <mj-html-attribute name="mc:edit">desc_content</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".cta_button td">
        <mj-html-attribute name="mc:edit">cta_button</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".details_headline_1 div">
        <mj-html-attribute name="mc:edit">details_headline_1</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".details_content_1 div">
        <mj-html-attribute name="mc:edit">details_content_1</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".details_image_1 img">
        <mj-html-attribute name="mc:edit">details_image_1</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".details_headline_2 div">
        <mj-html-attribute name="mc:edit">details_headline_2</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".details_content_2 div">
        <mj-html-attribute name="mc:edit">details_content_2</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".details_image_2 img">
        <mj-html-attribute name="mc:edit">details_image_2</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".footer_logo img">
        <mj-html-attribute name="mc:edit">footer_logo</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".social_claim div">
        <mj-html-attribute name="mc:edit">social_claim</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".features_headline div">
        <mj-html-attribute name="mc:edit">features_headline</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".features_subline_1 div">
        <mj-html-attribute name="mc:edit">features_subline_1</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".features_content_1 div">
        <mj-html-attribute name="mc:edit">features_content_1</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".features_subline_2 div">
        <mj-html-attribute name="mc:edit">features_subline_2</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".features_content_2 div">
        <mj-html-attribute name="mc:edit">features_content_2</mj-html-attribute>
      </mj-selector>

      <mj-selector path=".disclaimer_note div">
        <mj-html-attribute name="mc:edit">disclaimer_note</mj-html-attribute>
      </mj-selector>

    </mj-html-attributes>

    <mj-style>
      body {
        min-width: 600px;
        -webkit-text-size-adjust: 100%;
        -webkit-font-smoothing: antialiased;
      }

      .desc_keypoints div span {
        border-bottom: solid #f4b400 4px;
        padding-top:5px;
        padding-right:0;
        padding-bottom: 2px;
        padding-left:0;
        color: #000;
        font-weight:bold;
        font-size: 18px;
        text-decoration: none;
      }
      </mj-style>
  </mj-head>
  <mj-body>
    <mj-section>
      <mj-column vertical-align="middle">
        <mj-image width="100px" align="left" src="https://via.placeholder.com/100x32/50" css-class="nav_logo" alt="Logo" padding-bottom="0px" padding-left="20px" padding-top="0"></mj-image>
      </mj-column>
      <mj-column vertical-align="middle">
        <mj-text align="center" font-size="11px" padding-bottom="0px" padding-top="0" color="#646464" align="center" css-class="nav_claim"><span style="white-space: nowrap;">Meet our 3rd generation of ACME Inc. stuff</span></mj-text>
