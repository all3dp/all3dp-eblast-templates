  "scripts": {
    "clean": "rimraf .output && mkdirp .output",
    "test": "mjml --validate ./src/*.mjml",
    "build": "pnpm clean && mjml ./src/*.mjml -o ./.output/",
    "dev": "pnpm run clean && mjml -w ./src/*.mjml -o ./.output/"
  },
  "engines": {
    "node": "18.x",
    "pnpm": "7.x"
  },
  "dependencies": {
    "mjml": "^4.13.0"
  },
